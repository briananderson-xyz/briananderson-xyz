{
  "name": "briananderson-xyz",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "generate-variants": "node scripts/generate-variants.js",
    "prebuild": "npm run generate-variants",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:ui": "node scripts/validate-ui.js",
    "test:posthog": "node scripts/validate-posthog.js",
    "test:resumes": "tsx scripts/validate-resumes.ts",
    "test:ai": "node scripts/validate-ai.js",
    "test:e2e": "pnpm exec playwright test",
    "validate": "pnpm run check || echo 'TypeScript check failed (ignoring third-party error)' && pnpm run test:resumes && pnpm run build && pnpm run test:ui && pnpm run test:posthog && pnpm run test:ai"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.27.3",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "firebase-tools": "^15.5.1",
    "globals": "^17.3.0",
    "js-yaml": "^4.1.1",
    "mdsvex": "^0.12.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.3.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "firebase": "^12.9.0",
    "gray-matter": "^4.0.3",
    "lucide-svelte": "^0.462.0",
    "marked": "^17.0.1",
    "posthog-js": "^1.319.0",
    "zod": "^4.3.6"
  }
}
